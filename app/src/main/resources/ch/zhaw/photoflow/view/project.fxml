<?import javafx.scene.layout.*?>

<?import ch.zhaw.photoflow.controller.PhotoController ?>


<fx:root type="javafx.scene.layout.Pane" xmlns:fx="http://javafx.com/fxml">
	<BorderPane>
		<top>
	      <Pane fx:id="projectPane" layoutX="1.0" layoutY="10.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="75.0" prefWidth="936.0" style="-fx-border-style: solid;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
	         <children>
	            <TextField fx:id="projectNameField" layoutX="8.0" layoutY="27.0" text="Project Name" />
	            <MenuButton fx:id="todoButton" layoutX="507.0" layoutY="27.0" mnemonicParsing="false" prefHeight="13.0" prefWidth="112.0" text="MenuButton">
